{
  "name": "directus-extension-mikser",
  "version": "1.1.4",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "index.js",
    "host": "^9.2.1"
