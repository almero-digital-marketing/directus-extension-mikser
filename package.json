{
  "name": "directus-extension-mikser",
  "version": "1.0.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-hook"
  ],
  "directus:extension": {
    "type": "hook",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^9.2.1"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "scripts": {
    "build": "directus-extension build",
    "start": "npm run build -- --watch --no-minify --sourcemap",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/almero-digital-marketing/directus-extension-mikser.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/almero-digital-marketing/directus-extension-mikser/issues"
  },
  "homepage": "https://github.com/almero-digital-marketing/directus-extension-mikser#readme",
  "devDependencies": {
    "@directus/extensions-sdk": "^9.22.1"
  },
  "dependencies": {
    "deepdash": "^5.3.9",
    "fast-glob": "^3.2.12",
    "hasha": "^5.2.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "throttle-debounce": "^5.0.0"
  }
